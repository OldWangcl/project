<view class='columu' wx:if='{{kfhidden}}'>
<ad unit-id="adunit-c4e5d85db1c56fc6"></ad>
  <view class='coloum-class' wx:for='{{columnUrls}}' wx:key='item.id' catchtap='onColoumTap' data-tyname='{{item.tyname}}' data-id='{{item.c_typeid}}'>
      <view class='coloum-tw' >
        <image src='{{item.img}}' class='img' mode='aspectFill'></image>
        <text class='text'>{{item.tyname}}</text>
      </view>
  </view>
</view>