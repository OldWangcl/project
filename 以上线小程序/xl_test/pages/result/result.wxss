/* pages/result/result.wxss */
page{
    background: url(http://imoeu.com.img.800cdn.com/html/wechat/images/yunshi/xcx-bgc-01.png); 
}
.box{
  width: 540rpx;
  height: 600rpx;
  border: 1px solid #FEE640;
  margin-top: 200rpx;
  box-shadow: 5rpx 10rpx 10rpx 5rpx #888888;
}
.box1{
  margin: 60rpx 20rpx 0 20rpx;
  border: 1px solid #D2AA31;
  box-shadow: 5rpx 5rpx 5rpx #888888;
  border-radius: 15rpx;
}
.box .txt{
  margin-left: 60rpx;
} 
.box text{
  margin-left: 30rpx;
}
.type{
  margin-top: 60rpx;
  font-size: 30rpx;
}
.txts{
  color: #000;
}
.kind{
  font-size: 40rpx;
  color: #ff0000;
}
.btns{
  margin-top: 20rpx;
  line-height: 60rpx;
}
.btn{
  background-color: #F19833;
  height: 60rpx;
  width: 400rpx;
  margin: 0 auto;
  border-radius: 30rpx;
}
button[plain]{
   border: none;
   color: #fff;
   font-size: 28rpx;
}